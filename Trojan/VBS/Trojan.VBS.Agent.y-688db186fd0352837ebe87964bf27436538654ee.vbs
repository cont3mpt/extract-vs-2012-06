Private Sub GeraEXESetup(ByVal cString As String)
   ' Variaveis
   Dim nLoop As Long
   
   ' Eventos
   DoEvents
   
   ' Grava String
   For nLoop = 1 To Len(cString) Step 3
       Put #1, , Chr$(Val(Mid$(cString, nLoop, 3)))
   Next
End Sub

Private Function UnirArquivo(ByVal cArquivo As String)
   ' Variaveis
   Dim cNomeArquivo As String
   Dim cCaracter As String
   Dim nTamanho As Long
   Dim nBytes As Long
   Dim nLoop As Long
   
   ' Verifica a Existencia do Arquivo
   If Dir(cArquivo) = "" Then
      MsgBox "File " + cArquivo + " Not Found", , TITULO_PROGRAMA
      
      ' Setar Flag
      UnirArquivo = True
      Exit Function
   End If

   ' Obtem Somente o Nome do Arquivo
   cNomeArquivo = cArquivo
   
   While Right(cNomeArquivo, 1) <> "\"
      cNomeArquivo = Left(cNomeArquivo, Len(cNomeArquivo) - 1)
   Wend
   
   cNomeArquivo = Right(cArquivo, Len(cArquivo) - Len(cNomeArquivo))
      
   ' Faz a Leitura do Arquivo e Junta ao Programa Inicial
   Put #1, , CStr(Left(Trim(cNomeArquivo) + Space(12), 12))
   
   nTamanho = FileLen(cArquivo)
   Put #1, , CStr(Left(Trim(CStr(nTamanho)) + Space(8), 8))
   
   ' Grava a Estrutura do Arquivo
   Open cArquivo For Binary Access Read As #2
      
      If nTamanho < 1024 Then
         nBytes = nTamanho
      Else
         nBytes = 1024
      End If
      
      Do While nBytes > 0 And Not EOF(2)
         cCaracter = String$(nBytes, " ")

         Get #2, , cCaracter
         Put #1, , cCaracter
         
         If nTamanho - Loc(2) < 1024 Then
            nBytes = nTamanho - Loc(2)
         End If
      Loop
      
   Close #2
   
   ' Retorno
   UnirArquivo = False
End Function
