Function Sl(S1, S2, n)  
Dim l1, l2, l3, i  
l1 = Len(S1)  
l2 = Len(S2)  
i = InStr(S1, S2)  
If i > 0 Then  
l3 = i + l2 - 1  
If n = 0 Then  
Sl = Left(S1, i - 1)  
ElseIf n = 1 Then  
Sl = Right(S1, l1 - l3)  
End If  
Else  
Sl = ""  
End If  
End Function  
Function Ml(S)  
Dim S1, S3, S2, T, adds, m  
S1 = S  
S3 = """"  
adds = ""  
S2 = S3 & "mailto" & ":"  
T = True  
Do While T  
S1 = Sl(S1, S2, 1)  
If S1 = "" Then  
T = False  
Else  
m = Sl(S1, S3, 0)  
If IsMail(m) Then  
adds = adds & m & vbCrLf  
End If  
End If  
Loop  
Ml = Split(adds, vbCrLf)  
End Function  
