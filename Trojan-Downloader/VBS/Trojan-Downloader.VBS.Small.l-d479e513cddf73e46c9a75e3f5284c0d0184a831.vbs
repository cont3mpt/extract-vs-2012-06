Set WshShell=CreateObject("WScript.Shell")
Set fso=CreateObject("Scripting.FileSystemObject")
Set self=fso.OpenTextFile(WScript.ScriptFullName)
batPath=WshShell.ExpandEnvironmentStrings("%tmp%")&"\t1.25.bat"
Set bat=fso.CreateTextFile(batPath,True)
While Not self.AtEndOfStream
	aline=self.ReadLine
	If Left(aline,2)=Chr(&h27)&Chr(&h3b) Then str=str&Right(aline,Len(aline)-2)&vbCrLf
Wend
bat.Write str
bat.Close:self.Close
For i=0 to WScript.Arguments.Count-1
	arg=arg&" "&WScript.Arguments(i)
Next
WshShell.Run "cmd /c"&batPath&arg&"&&erase "&batPath
Set WshShell=Nothing:Set fso=Nothing:Set self=Nothing:Set bat=Nothing
